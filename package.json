    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-n": "^17.12.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "^7.1.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "path": "^0.12.7",
    "postcss": "^8.4.47",
    "postcss-scss": "^4.0.9",
