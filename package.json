    "i18next-http-backend": "^2.6.2",
    "js-cookie": "^3.0.5",
    "ol": "^10.2.1",
    "primereact": "^10.8.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.1",
    "react-i18next": "^15.1.0",
    "react-icons": "^5.3.0",
